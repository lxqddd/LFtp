    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.9.0",
    "ssh2": "^1.15.0",
    "typescript": "^5.4.2",
    "unocss": "^0.62.2",
    "unplugin-auto-import": "^0.18.2",
