  "version": "0.0.1",
  "private": true,
  "description": "a ftp client",
  "author": "向阳",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "main": "dist-electron/main/index.js",
